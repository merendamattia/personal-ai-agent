# Google Configuration
# Get API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MODEL=gemini-2.5-flash

# OpenAI Configuration
# Get API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=<your-api-key-here>
OPENAI_MODEL=gpt-4-turbo

# Telegram Bot Configuration
# Get your bot token from: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=<your-telegram-bot-token-here>

# Web Fetch Configuration
# Maximum tokens to truncate web content to (prevents token overflow)
# Increase for more detailed content, decrease for faster processing
MAX_TOKENS=150000

# Web Fetch Timeout
# Timeout in seconds for fetching web pages
WEB_FETCH_TIMEOUT=15

COMPOSE_BAKE=true
